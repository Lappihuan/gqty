{
  "name": "@gqty/vue",
  "version": "0.0.1",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "start:with:example": "bob-esbuild watch --onSuccess \"pnpm -r --filter vue-example dev\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "vue-demi": "^0.11.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/vue": "^5.8.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.173",
    "bob-esbuild-cli": "^1.0.1",
    "get-port": "^5.1.1",
    "gqty": "workspace:^1.1.3",
    "graphql": "^15.5.3",
    "jest": "^27.2.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "size-limit": "^5.0.3",
    "test-utils": "workspace:^0.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vue": "^2.6.0",
    "@vue/composition-api": "^1.1.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.1.0",
    "gqty": "workspace:^1.1.2",
    "graphql": "^15.5.2",
    "vue": "^2.6.14 || ^3.0.11"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "@vue/composition-api": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqty-react.esm.js",
      "limit": "10 KB"
    }
  ]
}
